{
  "name": "@upriser.ai/widget",
  "version": "1.1.0",
  "description": "A standalone JavaScript widget for integrating Upriser ElevenLabs ConvAI into any website",
  "main": "dist/upriser-widget.js",
  "module": "dist/upriser-widget.esm.js",
  "types": "dist/upriser-widget.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:raw && npm run build:umd && npm run build:esm && npm run build:types",
    "build:raw": "node scripts/build-raw.js",
    "build:umd": "node scripts/build-umd.js",
    "build:esm": "node scripts/build-esm.js",
    "build:types": "node scripts/build-types.js",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint tests/ scripts/ --ext .js",
    "lint:check": "eslint tests/ scripts/ --ext .js",
    "lint:fix": "eslint tests/ scripts/ --ext .js --fix",
    "dev": "node scripts/dev-server.js"
  },
  "keywords": [
    "widget",
    "chat",
    "conversational-ai",
    "elevenlabs",
    "upriser",
    "convai",
    "voice",
    "ai-assistant"
  ],
  "author": "vbrazo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vbrazo/upriser-agents.git"
  },
  "bugs": {
    "url": "https://github.com/vbrazo/upriser-agents/issues"
  },
  "homepage": "https://github.com/vbrazo/upriser-agents#readme",
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs"
}
